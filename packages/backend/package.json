{
  "name": "khajanchi-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prestart": "pnpm run build",
    "start": "node --env-file=.env dist/index.js",
    "build": "tsc",
    "dev": "concurrently \"tsc --watch\" \"node --env-file=.env.development --watch dist/index.js\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "test": "tsc && node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "concurrently \"tsc --watch\" \"jest --watch\"",
    "test:coverage": "tsc && jest --coverage",
    "db:migration:generate": "npx sequelize-cli --migrations-path src/sequelizeCli/migrations migration:generate --name",
    "db:seed:generate": "npx sequelize-cli seed:generate --seeders-path src/sequelizeCli/seeders/ --name",
    "db:migrate": "pnpm run build && npx sequelize-cli db:migrate",
    "db:migrate:undo": "pnpm run build && npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "pnpm run build && npx sequelize-cli db:migrate:undo:all",
    "db:seed:all": "pnpm run build && npx sequelize-cli db:seed:all",
    "db:seed:undo": "pnpm run build && npx sequelize-cli db:seed:undo",
    "db:seed:undo:all": "pnpm run build && npx sequelize-cli db:seed:undo:all"
  },
  "keywords": [],
  "author": "Gnanesh Puttaswamy",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.1",
    "@types/nodemailer": "^6.4.17",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "openai": "^4.86.1",
    "sequelize": "^6.37.5",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  }
}
